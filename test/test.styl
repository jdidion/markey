@import 'nib'
@import 'lost'
@import 'typographic'

header-font = franklin
body-font = franklin
min-font = 20px

typographic()

edit(green)

header
  text-align center

article
  clearfix()
  
article > div
  column('1/3')

div#left
  width 15%

div#left > section
  row('1/2')

div#middle
  width 70%

div#middle > section
  column-count 3

div#right
  width 15%

div#right > section
  row('1/2')


